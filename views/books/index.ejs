<h2>Search Books</h2>

<br>

<form action="/books" method="GET">
    <label>Title</label>
    <input type="text" name="title" value="<%= searchOptions.title %> ">
    <button type="submit">Search</button>
</form>

<br>

<div>
    <% books.forEach(book => { %>
        <div><img height="150" width="100" src="<%= book.coverImagePath %>"></div>
    <% }) %> 
</div>

