<h2>Recently added Titles</h2>

<div>
    <% books.forEach(book => { %>
        <a href="/books/<%= book.id %>">
            <img height="150" width="100" src="<%= book.coverImagePath %>">
        </a>
    <% }) %>
</div>

<div>
    <% if (books = []) { %>
        <h4><%= displayText %></h4>
    <% } %>
</div>
